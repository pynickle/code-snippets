{
    "os.walk": {
        "prefix": "for",
        "body": [
            "for root, dirs, files in os.walk(${1:path}):"
        ],
        "description": "use os to walk the path"
    },
    "tkinter.Tk": {
        "prefix":"Tk()",
        "body": [
            "root = tkinter.Tk()"
        ],
        "description": "create a tkinter window"
    },
    "root.mainloop": {
        "prefix": "mainloop()",
        "body": [
            "root.mainloop()"
        ],
        "description": "let tkinter window run"
    },
    "for enumerate": {
        "prefix": "for",
        "body": [
            "for index, nums in enumerate(${1:iterable}):"
        ],
        "description": "enumerate the items in the iterable"
    },
    "dict.items": {
        "prefix": "for",
        "body": [
            "for key, value in ${1:dict}.items():"
        ],
        "description": "get every key and value in the dict"
    },
    "parser": {
        "prefix": "ArgumentParser()",
        "body": [
            "parser = argparse.ArgumentParser()"
        ],
        "description": "create an argument parser from argparse"
    },
    "args": {
        "prefix": "parse_args",
        "body": [
            "args = parser.parse_args"
        ],
        "description": "parse the argument from argparse"
    },
    "add_argument": {
        "prefix": "add_argument",
        "body": [
            "parser.add_argument(${1:name})"
        ],
        "description": "add an argument from argparse"
    },
    "numpy": {
        "prefix": "numpy",
        "body": [
            "import numpy as np"
        ],
        "description": "import numpy module as np"
    },
